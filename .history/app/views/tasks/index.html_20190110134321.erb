<%= stylesheet_link_tag "stylesheet/todo", media "all", => true%>
<h1>Your Tasks</h1>
<table>
    <thead>
        <tr>
            <th>Task Name</th>
        </tr>
    </thead>
    <tbody>
        <% @tasks.each do |task| %>
            <tr>
                <td><%= task.title %></td>
                <td><%= link_to 'edit', edit_task_path(task) %></td>
                <td><%= link_to 'delete', task_path(task), method: :delete %></td>
            </tr>
        <% end %>
    </tbody>
</table>
<%= link_to 'Add', new_task_path %>